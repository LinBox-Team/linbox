package LinBoxComputingServer;

import java.io.File;

/** A simple interface to hold machine-specific locations required to run the
  * server.
  */
public interface Locations {
	int port = ${PORT};
	int TIMEOUT = ${TIMEOUT};
	File LINBOX_FOLDER = new File( "${LINBOX_FOLDER}" );
	File TEMP_DIR = new File( "${COMPUTING_SERVER_TEMP_DIR}" );
}
