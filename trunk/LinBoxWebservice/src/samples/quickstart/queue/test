// test.java
// Matthew Fendt Summer '07-'08 Research
// Tests the queue

import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
import java.lang.Thread;

public class test
{
    public static void main(String[] args)
    {
	//String host = (args.length < 1) ? null : args[0];
	try{

	    Registry registry = LocateRegistry.getRegistry();
	    QueueRequest stub = (QueueRequest)registry.lookup("QueueRequest");
	    String response = stub.enqueue("Rank", "m1", "100");
	    System.out.println("Response: " + response);
	    Thread.currentThread().sleep(2000);

	    stub = (QueueRequest)registry.lookup("QueueRequest");
	    response = stub.enqueue("Det" , "m2", "100");
	    System.out.println("\nResponse: " + response);
	    Thread.currentThread().sleep(2000);

	    stub = (QueueRequest)registry.lookup("QueueRequest");
	    response = stub.enqueue("Val",  "m3", "102");
	    System.out.println("\nResponse: " + response);
	    Thread.currentThread().sleep(7000);

	    stub = (QueueRequest)registry.lookup("QueueRequest");
	    response = stub.enqueue("SNF",  "m4", "101");
	    System.out.println("\nResponse: " + response);
	    Thread.currentThread().sleep(2000);

	    stub = (QueueRequest)registry.lookup("QueueRequest");
	    response = stub.enqueue("Rank", "m5", "100");
	    System.out.println("\nResponse: " + response);
	    
	    //	    q.printQueue();
	}
	
	catch (Exception e) 
	    {
		System.err.println("Client exception: " + e.toString());
		e.printStackTrace();
	    }
    }
    
}
