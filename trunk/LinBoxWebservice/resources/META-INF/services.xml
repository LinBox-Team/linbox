<service name="TransferAgent" scope="application">
    <messageReceivers>
        <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
                         class="samples.quickstart.service.adb.TransferAgentMessageReceiverInOut"/>
    </messageReceivers>
    <parameter locked="false" name="ServiceClass">samples.quickstart.service.adb.TransferAgentSkeleton
    </parameter>

    <operation name="rank" mep="http://www.w3.org/2004/08/wsdl/in-out">
        <actionMapping>urn:rank</actionMapping>
        <outputActionMapping>http://quickstart.samples/TransferAgentPortType/getPriceResponse</outputActionMapping>
    </operation>

    <operation name="estimateRankTime" mep="http://www.w3.org/2004/08/wsdl/in-out">
        <actionMapping>urn:estimateRankTime</actionMapping>
        <outputActionMapping>http://quickstart.samples/TransferAgentPortType/getPriceResponse</outputActionMapping>
    </operation>

    <operation name="determinant" mep="http://www.w3.org/2004/08/wsdl/in-out">
        <actionMapping>urn:det</actionMapping>
        <outputActionMapping>http://quickstart.samples/TransferAgentPortType/getPriceResponse</outputActionMapping>
    </operation>

    <operation name="valence" mep="http://www.w3.org/2004/08/wsdl/in-out">
        <actionMapping>urn:val</actionMapping>
        <outputActionMapping>http://quickstart.samples/TransferAgentPortType/getPriceResponse</outputActionMapping>
    </operation>

    <operation name="trace" mep="http://www.w3.org/2004/08/wsdl/in-out">
        <actionMapping>urn:trace</actionMapping>
        <outputActionMapping>http://quickstart.samples/TransferAgentPortType/getPriceResponse</outputActionMapping>
    </operation>

    <operation name="smithNormalForm" mep="http://www.w3.org/2004/08/wsdl/in-out">
        <actionMapping>urn:smithNormalForm</actionMapping>
        <outputActionMapping>http://quickstart.samples/TransferAgentPortType/getPriceResponse</outputActionMapping>
    </operation>

    <operation name="getMachineSpecs" mep="http://www.w3.org/2004/08/wsdl/in-out">
        <actionMapping>urn:getMachineSpecs</actionMapping>
        <outputActionMapping>http://quickstart.samples/TransferAgentPortType/getPriceResponse</outputActionMapping>
    </operation>
</service>
